import sys\
\
class Solution:\
    def minDiffInBST(self, root: TreeNode) -> int:\
        def preOrder(node):\
            if node == None:\
                return\
            preOrder(node.left)\
            self.arr.append(node.val)\
            preOrder(node.right)\
        \
        result = sys.maxsize\
        self.arr = []\
        preOrder(root)\
        self.arr.sort()\
        for i in range(len(self.arr) - 1):\
            dif = abs(self.arr[i] - self.arr[i + 1])\
            if dif < result:\
                result = dif\
        return result\
