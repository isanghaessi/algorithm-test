class Solution:\
    def isPalindrome(self, head: ListNode) -> bool:\
        nums = collections.deque()\
        node = head\
        while node:\
            nums.append(node.val)\
            node = node.next\
        while len(nums) > 1:\
            if nums.popleft() != nums.pop():\
                return False\
        return True\
