1.\uc0\u47532 \uc0\u49828 \uc0\u53944 \uc0\u47196  \uc0\u48320 \uc0\u54872 \
class Solution:\
    def isPalindrome(self, head: ListNode) -> bool:\
        nums = []\
        node = head\
        while node:\
            nums.append(node.val)\
            node = node.next\
        while len(nums) > 1:\
            if nums.pop(0) != nums.pop():\
                return False\
        return True\
\
