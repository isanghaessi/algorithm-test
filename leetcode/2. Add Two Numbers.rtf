class Solution:\
    def toList(self, head: ListNode) -> List[str]:\
        result = []\
        while head:\
            result.append(str(head.val))\
            head = head.next\
        return result\
    def addTwoNumbers(self, l1: ListNode, l2: ListNode) -> ListNode:\
        result = None\
        left = int(''.join(self.toList(l1)[::-1]))\
        right = int(''.join(self.toList(l2)[::-1]))\
        for c in str(left + right):\
            result = ListNode(val = int(c), next = result)\
        return result\
