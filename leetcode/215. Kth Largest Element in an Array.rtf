class Solution:\
    def findKthLargest(self, nums: List[int], k: int) -> int:\
        nums = [[-n, n]for n in nums]\
        heapq.heapify(nums)\
        for _ in range(k):\
            result = heapq.heappop(nums)[1]\
        return result\
